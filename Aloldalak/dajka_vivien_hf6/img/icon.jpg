<script id="js-etoro-cookie-consent">
        window.eToroCookieConsent = window.eToroCookieConsent || {};

        function otHandleLearnMore() {
            if (window.eToroCookieConsent.isEventLearnMoreAttached) {
                return;
            }

            const link_more = document.getElementById('otet-show-long-text');
            if (link_more) {
                link_more.addEventListener('click', function (e) {
                    e.preventDefault();
                    document.getElementById('otet-short-text').remove();
                    document.body.classList.add('show-otet-long-text');
                });
            }

        }

        const otTimerId = setInterval(function () {
            if (!window.eToroCookieConsent.isEventLearnMoreAttached) {
                if (!window.OneTrust || typeof OneTrust.GetDomainData !== 'function') {
                    return
                }

                const otData = OneTrust.GetDomainData();
                if (!otData.IsBannerLoaded) {
                    // add event listener
                    document.querySelector('html').addEventListener('otloadbanner', function () {
                        requestAnimationFrame(function () {
                            const otBanner = document.getElementById('onetrust-banner-sdk');
                            if (otBanner) {
                                otHandleLearnMore();
                                window.eToroCookieConsent.isEventLearnMoreAttached = true;
                            }
                        });
                    }, {once: true});
                } else {
                    //banner already loaded
                    //find "Learn More" and attach event
                    const otBanner = document.getElementById('onetrust-banner-sdk');
                    if (otBanner) {
                        otHandleLearnMore();
                        window.eToroCookieConsent.isEventLearnMoreAttached = true;
                    } else { //if IsBannerLoaded but there are conditions to not show it
                        clearInterval(otTimerId);
                    }
                }
            } else {
                clearInterval(otTimerId);
            }
        }, 100);

        setTimeout(function () {
            clearInterval(otTimerId);
        }, 10000);

    </script>
