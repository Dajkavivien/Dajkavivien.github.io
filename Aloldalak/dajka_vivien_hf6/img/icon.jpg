<script id="js-etoro-layout">
    (function () {
        const topfixed_content = document.getElementById('topfixed-content');
        const before_topmenu = document.getElementById('before-topmenu');
        const static_on_top = document.getElementById('static-on-top');
        const body = document.body;

        function setCoverOffset() {
            if (!before_topmenu) {
                return;
            }
            // -14 is for allowing to show border radius on top of cover
            const offset_for_cover = (static_on_top) ?
                before_topmenu.getBoundingClientRect().height + static_on_top.getBoundingClientRect().height - 14
                : before_topmenu.getBoundingClientRect().height ;
            body.style.setProperty('--cover-offset', offset_for_cover + "px");
        }

        function setTopOffset() {
            if (!topfixed_content) {
                return;
            }

            body.style.setProperty('--top-offset', topfixed_content.getBoundingClientRect().height + "px");
        }

        function setCSSVariables() {
            const cover_height = (static_on_top) ? window.innerHeight - static_on_top.getBoundingClientRect().height
                : window.innerHeight ;
            body.style.setProperty('--inner-height', cover_height + "px");
            setTopOffset();
            setCoverOffset();
        }

        const resizeObserver = new ResizeObserver(entries => {
            setCSSVariables();
        });

        setCSSVariables();
        if (topfixed_content) {
            resizeObserver.observe(topfixed_content);
        }

        if (before_topmenu) {
            resizeObserver.observe(before_topmenu);
        }

        if (static_on_top) {
            resizeObserver.observe(static_on_top);
        }
    })();
</script>
