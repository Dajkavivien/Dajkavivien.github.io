<script id="ab-tests">
    window.etoro_ab_tests = {};

    function ab_test_balance_cover() {
        if (isABTestsDisabled) {
            return;
        }

        if (etoro_hp.settings.locale !== 'en-gb' || etoro_hp.settings.uri_path_after_lang !== '') {
            return;
        }

        const urlParams = new URLSearchParams(window.location.search);

        const balance_cover_test = urlParams.get('cover');

        window.etoro_ab_tests.ab_balance = (balance_cover_test && balance_cover_test === 'balance') ? 'balance' : 'original';
    }

    ab_test_balance_cover();
</script>
